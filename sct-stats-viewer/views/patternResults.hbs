<div class="col-md-12">
    <div class="card" style="overflow: auto;">
        <div class="card-height-indicator"></div>
        <div class="card-content">
            <div class="card-body">
                <h2>Pattern: {{pattern}}</h2>
                <p>{{count}} concepts</p>
                <a class="btn btn-material-light-blue pull-right" style="margin: 0px;" href="javascript:loadAnalyticsResultsIndex();" role="button">Back to Patterns</a>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="bs-component">
                            <table class="table table-striped">
                                <thead>
                                <tr>
                                    <th class="col-md-3">ConceptId</th>
                                    <th class="col-md-4">Term</th>
                                    <th class="col-md-5">Explanation</th>
                                </tr>
                                </thead>
                                <tbody>
                                {{#each results}}
                                    <tr class="small">
                                        <td class="">{{conceptId}}</td>
                                        <td class="">{{term}}</td>
                                        <td class="">{{matchDescription}}</td>
                                    </tr>
                                {{/each}}
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <!--<td colspan="6">(*) Percentage of existing concepts in hierarchy which are Sufficiently Defined (SD). For the root concept the value includes all SNOMED CT.<br>-->
                                        </td>
                                    </tr>
                                </tfoot>
                            </table>
                        </div><!-- /example -->
                    </div>
                </div>
            </div>
            <footer class="card-footer">
                <!--<button class="btn btn-flat">Share</button>-->
                <!--<button class="btn btn-flat btn-warning">Learn More</button>-->
            </footer>
        </div>
    </div>
</div>
